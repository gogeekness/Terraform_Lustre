#test.tf
provider "aws" {
  region = "eu-central-1"
}

# Simple test resource - an S3 bucket
resource "aws_s3_bucket" "test" {
  bucket = "test-bucket-rdeseke-${random_string.suffix.result}"
  force_destroy = true
}

resource "random_string" "suffix" {
  length  = 8
  special = false
  upper   = false
}